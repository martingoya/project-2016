@{
    ViewBag.Title = "NewBorn";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <script src="~/Scripts/general-functions.js"></script>
    <script>
        window.onload = function () {
            initGallery(3);
        };

        $(function () {
            $('.thumbs a').touchTouch();
            var $container = $('#container');
        });
    </script>
</head>

<body>
<div id="content" class="content">
    <div class="row_1_Section">
        <div class="container">
            <p class="title2" style="margin-bottom: 10px; padding-top:25px">Texto NewBorn</p>
            <p class="title2"></p>
            @*<a href="#" class="btn btn-default btn-lg btn1">more</a>*@
        </div>
    </div>
    <!--=============================loading===============================-->
    <div id="loading"></div>
    <!--==============================row6=================================-->
    <div class="row_6">
        <div class="container">
            <div class="row">
        		<div class="col-lg-12 col-md-12 col-sm-12">
                    <div id="options" class="clearfix" style="display:none">
                      <ul id="filters" class="pagination option-set clearfix" data-option-key="filter">
                        <li><a href="#filter" data-option-value="*" class="selected">Show all</a></li>
                        <li><a href="#filter" data-option-value=".people">people</a></li>
                        <li><a href="#filter" data-option-value=".macro ">macro</a></li>
                        <li><a href="#filter" data-option-value=".nature">nature</a></li>
                        <li><a href="#filter" data-option-value=".night">night</a></li>
                        <li><a href="#filter" data-option-value=".panoramic">panoramic</a></li>
                        <li><a href="#filter" data-option-value=".sports">sports</a></li>
                      </ul>
                    </div><!-- #options -->
                    <div class="containerExtra">
                        <div id="container" class="clearfix">
                            <div class="thumbs">
                                @*NewBorn*@
                                @for (int i = 1; i <= 33; i++)
                                {
                                    <div class="element transition people" data-category="transition">
                                        <a href="~/Content/Fotos/NewBorn/NewBorn (@i).jpg">
                                            <figure class="img-polaroid" title=""><img src="~/Content/Fotos/NewBorn/_Portadas/NewBorn (@i).jpg" alt=""></figure>
                                        </a>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="thumbs" style="display:none">
                    </div>
	            </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>